package operator;

public class Ex1 {
	public static void main(String[] args) {
		
		/*
		 *  단항연산자 a++,a--,++a,--a (증감연산자) 후행연산자는 실행문 실행후 증감
		 *  산술연산자 + - * / % : 나머지 연산자
		 *  대입연산자 =, 복합대입연산자 +=,-=,*=,/=,%=
		 *  비교연산자 ==, !=, <,>
		 *  논리연산자 &&,||,!,^ (true^false=true)
		 *  삼항연산자 조건식? true일때 값:false일때 값
		 * 
		*/
		
		int i = 3;
		i++;
		System.out.println(i);
		++i;
		System.out.println(i);
		System.out.println(++i);
		System.out.println(i++);
		System.out.println(i);
		
	}
	
}
