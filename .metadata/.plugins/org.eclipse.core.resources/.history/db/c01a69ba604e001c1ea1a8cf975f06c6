package variable;

public class Test4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		/*
		 * 구구단2단 출력
		 * printf() println()활용하여출력
		 * 
		 * 
		*/
		
		int multi = 2;
		int multi1 = multi*1;
		int multi2 = multi*2;
		int multi3 = multi*3;
		int multi4 = multi*4;
		int multi5 = multi*5;
		int multi6 = multi*6;
		int multi7 = multi*7;
		int multi8 = multi*8;
		int multi9 = multi*9;
		
		System.out.println("< "+ multi + "단 >");
		System.out.println(multi+" * 1 = "+multi1);
		System.out.println(multi+" * 2 = "+multi2);
		System.out.println(multi+" * 3 = "+multi3);
		System.out.println(multi+" * 4 = "+multi4);
		System.out.println(multi+" * 5 = "+multi5);
		System.out.println(multi+" * 6 = "+multi6);
		System.out.println(multi+" * 7 = "+multi7);
		System.out.println(multi+" * 8 = "+multi8);
		System.out.println(multi+" * 9 = "+multi9);
		System.out.println();
		System.out.println("----------------\n");
		
		System.out.printf("< %d단 >\n", multi);
		System.out.printf("%d * 1 = %d\n", multi, multi1);
		System.out.printf("%d * 2 = %d\n", multi, multi2);
		System.out.printf("%d * 3 = %d\n", multi, multi3);
		System.out.printf("%d * 4 = %d\n", multi, multi4);
		System.out.printf("%d * 5 = %d\n", multi, multi5);
		System.out.printf("%d * 6 = %d\n", multi, multi6);
		System.out.printf("%d * 7 = %d\n", multi, multi7);
		System.out.printf("%d * 8 = %d\n", multi, multi8);
		System.out.printf("%d * 9 = %d", multi, multi9);
		
		
		
	}

}
