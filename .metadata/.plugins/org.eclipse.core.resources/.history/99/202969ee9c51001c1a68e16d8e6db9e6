package review;

public class Ex1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		System.out.println("문자열"+"어떤타입이든"+0.5);//문자열은 어떤타입과 덧셈연산을 해도 문자열로 자동형변환
		System.out.println('1'+10); //문자타입은 더 튼 데이터다입으로 자동형변환된다.
		System.out.println('1'+(char)10); 
		System.out.println('1'+'2');
		System.out.println('1');
		
	}

}
