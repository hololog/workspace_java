package operator;

public class Ex5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * 
		 * 비교연산자 대소관계비교하여 true or false
		 * 
		 */

		int a = 10, b = 5;
		System.out.println("a>b인가 " + (a > b));
		System.out.println("a<b인가 " + (a < b));
		System.out.println("a>=b인가 " + (a >= b));
		System.out.println("a<=b인가 " + (a <= b));
		System.out.println("a!=b인가 " + (a != b));
		System.out.println("a==b인가 " + (a == b));
		
		//char 타입을 비교연산자에 사용시 정수갓을 비교
		System.out.println('A' >'B');
		System.out.println('A' <'B');
		System.out.println("===================");
	}

}
