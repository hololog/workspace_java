package operator;

public class Ex4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		/*
		 * 대입연산자 =
		 * 우변을 좌변에 대입
		 * 복합대입연산자 : 산술연산자를 먼저 수행후 대입연산자 수행
		 * 
		*/
		
		int a=100, b=10;
		
		
		
		b-=a;
		System.out.println(b);
		b-=a;
		System.out.println(b);
		b*=a;
		System.out.println(b);
		b/=a;
		System.out.println(b);
		b%=a;
		System.out.println(b);

		
		
		
	}

}
