package variable;

public class Ex3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		/*
		 * String 타입
		 * - 문자열을 표현하기위한 데이터 타입
		 * - 참조형 (Reference type)
		 * - 1개 이상이 문자열은 큰 따음표로 표현
		 * - null string 가능
		 * - 덧셈 연산자로 문자열 결합 가능
		*/
		String s1 =  "s"
		
	}

}
